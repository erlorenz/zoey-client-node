{
  "name": "@eldev/zoey-client-node",
  "version": "0.5.5",
  "description": "A client library for working with the Zoey API.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test": "vitest run unit",
    "test:e2e": "vitest run e2e -c vitest.config.e2e.ts",
    "test:watch": "vitest unit",
    "lint": "tsc",
    "coverage": "vitest run unit --coverage",
    "build": "npm run clean && npm run lint && tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erlorenz/zoey-client-node.git"
  },
  "author": "Erik Lorenz",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.8.9",
    "dotenv": "^16.3.1",
    "msw": "^2.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "dependencies": {
    "oauth-1.0a": "^2.2.6"
  },
  "peerDependencies": {
    "zod": "^3.20"
  },
  "engines": {
    "node": ">=18"
  }
}
