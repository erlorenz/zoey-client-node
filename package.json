{
  "name": "zoey-client-node",
  "version": "0.1.2",
  "description": "A client library for working with the Zoey API.",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "test:dev": "vitest",
    "test-node": "node --test --trace-warnings --env-file .env --loader tsx/esm test/node/*.ts",
    "lint": "tsc --noemit",
    "coverage": "vitest run --coverage",
    "build": "npm run lint && tsup src/index.ts --format cjs,esm --dts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erlorenz/zoey-client-node.git"
  },
  "author": "Erik Lorenz",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.6.0",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "tsup": "^7.2.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "oauth-1.0a": "^2.2.6",
    "zod": "^3.22.2"
  },
  "engines": {
    "node": ">= 18.0.0"
  }
}
